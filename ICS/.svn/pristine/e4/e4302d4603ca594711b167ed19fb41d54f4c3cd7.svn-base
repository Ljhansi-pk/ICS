    ''' <summary>
    ''' Add SAP Endurance To TRACS Data
    ''' </summary>
    ''' <param name="oTable">oTable</param>
    ''' <param name="stTRACS">stTRACS</param>
    ''' <remarks></remarks>
    Private Sub AddSAPEnduranceToTRACSData(ByVal oTable As System.Data.DataTable, ByRef stTRACS As ICS.Datasets.TRACStoICSDataset)
        Dim rowCount As Int16

        Dim iLowStep As Integer

        '   Add Endurance Header to Result Set
        If oTable.TableName = "EnduranceHdr" Then
            ConvertNulls(oTable)
            For rowCount = 0 To oTable.Rows.Count - 1
                Dim sTRACSHdrRow As ICS.Datasets.TRACStoICSDataset.EnduranceHdrRow = stTRACS.EnduranceHdr.NewEnduranceHdrRow
                sTRACSHdrRow.ProjectNum = oTable.Rows(rowCount).Item("ProjectNum")
                sTRACSHdrRow.TireNum = ConvertToShort(oTable.Rows(rowCount).Item("TireNum"))
                sTRACSHdrRow.TestSpec = oTable.Rows(rowCount).Item("TestSpec")
                sTRACSHdrRow.TestSKU = oTable.Rows(rowCount).Item("TestSKU")
                sTRACSHdrRow.CompletionDate = ConvertToDate(oTable.Rows(rowCount).Item("CompletionDate"))
                sTRACSHdrRow.DOTSerialNumber = oTable.Rows(rowCount).Item("DOTSerialNumber")
                sTRACSHdrRow.SerialDate = oTable.Rows(rowCount).Item("SerialDate")
                sTRACSHdrRow.PreCondStartDate = oTable.Rows(rowCount).Item("PreCondStartDate")
                sTRACSHdrRow.PreCondSartTemp = ConvertToShort(oTable.Rows(rowCount).Item("PreCondSartTemp"))
                sTRACSHdrRow.RimDiameter = ConvertToSingle(oTable.Rows(rowCount).Item("RimDiameter"))
                sTRACSHdrRow.RimWidth = ConvertToSingle(oTable.Rows(rowCount).Item("RimWidth"))
                sTRACSHdrRow.PreCondEndDate = oTable.Rows(rowCount).Item("PreCondEndDate")
                sTRACSHdrRow.PreCondEndTemp = ConvertToShort(oTable.Rows(rowCount).Item("PreCondEndTemp"))
                sTRACSHdrRow.InflationPressure = ConvertToShort(oTable.Rows(rowCount).Item("InflationPressure"))
                sTRACSHdrRow.BeforeDiameter = ConvertToSingle(oTable.Rows(rowCount).Item("BeforeDiameter"))
                sTRACSHdrRow.AfterDiameter = ConvertToSingle(oTable.Rows(rowCount).Item("AfterDiameter"))
                sTRACSHdrRow.BeforeInflation = ConvertToShort(oTable.Rows(rowCount).Item("BeforeInflation"))
                sTRACSHdrRow.AfterInflation = ConvertToShort(oTable.Rows(rowCount).Item("AfterInflation"))
                sTRACSHdrRow.WheelPosition = ConvertToShort(oTable.Rows(rowCount).Item("WheelPosition"))
                sTRACSHdrRow.WheelNum = ConvertToShort(oTable.Rows(rowCount).Item("WheelNum"))
                sTRACSHdrRow.LowInfEndTemp = ConvertToShort(oTable.Rows(rowCount).Item("LowInfEndTemp"))
                sTRACSHdrRow.LowInfTotalDistance = ConvertToSingle(oTable.Rows(rowCount).Item("LowInfTotalDistance"))
                sTRACSHdrRow.LowInfStartInflation = ConvertToShort(oTable.Rows(rowCount).Item("LowInfStartInflation"))
                sTRACSHdrRow.LowInfEndInflation = ConvertToShort(oTable.Rows(rowCount).Item("LowInfEndInflation"))
                sTRACSHdrRow.FinalTemp = ConvertToShort(oTable.Rows(rowCount).Item("FinalTemp"))
                sTRACSHdrRow.FinalDistance = ConvertToSingle(oTable.Rows(rowCount).Item("FinalDistance"))
                sTRACSHdrRow.FinalInflation = ConvertToShort(oTable.Rows(rowCount).Item("FinalInflation"))
                sTRACSHdrRow.PostCondStartDate = ConvertToDate(oTable.Rows(rowCount).Item("PostCondStartDate"))
                sTRACSHdrRow.PostCondEndDate = ConvertToDate(oTable.Rows(rowCount).Item("PostCondEndDate"))
                sTRACSHdrRow.PostCondEndTemp = ConvertToShort(oTable.Rows(rowCount).Item("PostCondEndTemp"))
                sTRACSHdrRow.PostCondTime = ConvertToSingle(oTable.Rows(rowCount).Item("PostCondTime"))
                If sTRACSHdrRow.PostCondTime > 32767 Then 'checking for invalid data from SAP.
                    sTRACSHdrRow.PostCondTime = 0
                End If
                sTRACSHdrRow.PassYN = oTable.Rows(rowCount).Item("PassYN")
                sTRACSHdrRow.AnalysisCode = oTable.Rows(rowCount).Item("AnalysisCode")
                sTRACSHdrRow.CompletedBy = oTable.Rows(rowCount).Item("CompletedBy")
                sTRACSHdrRow.Operation = oTable.Rows(rowCount).Item("OperationDescription")
                sTRACSHdrRow.MFGWWYY = oTable.Rows(rowCount).Item("SERIALDATE")
                sTRACSHdrRow.GTSPEC = oTable.Rows(rowCount).Item("GTSpec")
                stTRACS.EnduranceHdr.AddEnduranceHdrRow(sTRACSHdrRow)
            Next
        End If

        'Need to split out low inflation steps

        '   Add Endurance Detail to Result Set
        If oTable.TableName = "EnduranceDtl" Then
            iLowStep = 0
            ConvertNulls(oTable)
            For rowCount = 0 To oTable.Rows.Count - 1
                If ConvertToShort(oTable.Rows(rowCount).Item("TestStep")) <> 0 And _
                   oTable.Rows(rowCount).Item("TimeInMin") = "" And _
                    oTable.Rows(rowCount).Item("Speed") = "" And _
                    oTable.Rows(rowCount).Item("TotDistance") = "" Then
                    'skip steps that don't have values (steps being returned even though they weren't completed.jeseitz 6/25/2014
                Else

                    Dim sTRACSDtlRow As ICS.Datasets.TRACStoICSDataset.EnduranceDtlRow = stTRACS.EnduranceDtl.NewEnduranceDtlRow
                    sTRACSDtlRow.TestStep = ConvertToShort(oTable.Rows(rowCount).Item("TestStep"))
                    sTRACSDtlRow.TimeInMin = ConvertToShort(oTable.Rows(rowCount).Item("TimeInMin"))
                    sTRACSDtlRow.Speed = ConvertToSingle(oTable.Rows(rowCount).Item("Speed"))
                    sTRACSDtlRow.TotMiles = ConvertToSingle(oTable.Rows(rowCount).Item("TotDistance"))
                    sTRACSDtlRow.Load = ConvertToSingle(oTable.Rows(rowCount).Item("Load"))
                    sTRACSDtlRow.LoadPercent = ConvertToSingle(oTable.Rows(rowCount).Item("LoadPercent"))
                    sTRACSDtlRow.SetInflation = ConvertToShort(oTable.Rows(rowCount).Item("SetInflation"))
                    sTRACSDtlRow.AmbTemp = ConvertToShort(oTable.Rows(rowCount).Item("AmbTemp"))
                    sTRACSDtlRow.InfPressure = ConvertToShort(oTable.Rows(rowCount).Item("InfPressure"))
                    sTRACSDtlRow.StepCompletionDate = ConvertToDate(oTable.Rows(rowCount).Item("StepCompletionDate"))
                    'Don't add it if set inflation > 0 after the first step - this is the start of the low inflation
                    If iLowStep = 0 And (rowCount > 0 And sTRACSDtlRow.SetInflation > 0) Then
                        iLowStep = rowCount
                    End If
                    If iLowStep = 0 Then
                        stTRACS.EnduranceDtl.AddEnduranceDtlRow(sTRACSDtlRow)
                    End If
                End If
            Next
        End If

        '   Add Endurance Low Inflation Detail to Result Set
        If oTable.TableName = "EndLowInfDtl" Then
            If iLowStep > 0 Then
                ConvertNulls(oTable)
                For rowCount = iLowStep To oTable.Rows.Count - 1
                    If ConvertToShort(oTable.Rows(rowCount).Item("TestStep")) <> 0 And _
                       oTable.Rows(rowCount).Item("TimeInMin") = "" And _
                        oTable.Rows(rowCount).Item("Speed") = "" And _
                        oTable.Rows(rowCount).Item("TotDistance") = "" Then
                    Else

                        Dim sTRACSDtlRow As ICS.Datasets.TRACStoICSDataset.EndLowInfDtlRow = stTRACS.EndLowInfDtl.NewEndLowInfDtlRow
                        sTRACSDtlRow.TestStep = ConvertToShort(oTable.Rows(rowCount).Item("TestStep"))
                        sTRACSDtlRow.TimeInMin = ConvertToShort(oTable.Rows(rowCount).Item("TimeInMin"))
                        sTRACSDtlRow.Speed = ConvertToSingle(oTable.Rows(rowCount).Item("Speed"))
                        sTRACSDtlRow.TotMiles = ConvertToSingle(oTable.Rows(rowCount).Item("TotDistance"))
                        sTRACSDtlRow.Load = ConvertToSingle(oTable.Rows(rowCount).Item("Load"))
                        sTRACSDtlRow.LoadPercent = ConvertToSingle(oTable.Rows(rowCount).Item("LoadPercent"))
                        sTRACSDtlRow.SetInflation = ConvertToShort(oTable.Rows(rowCount).Item("SetInflation"))
                        sTRACSDtlRow.AmbTemp = ConvertToShort(oTable.Rows(rowCount).Item("AmbTemp"))
                        sTRACSDtlRow.InfPressure = ConvertToShort(oTable.Rows(rowCount).Item("InfPressure"))
                        sTRACSDtlRow.StepCompletionDate = ConvertToDate(oTable.Rows(rowCount).Item("StepCompletionDate"))
                        stTRACS.EndLowInfDtl.AddEndLowInfDtlRow(sTRACSDtlRow)
                    End If
                Next
            End If
        End If

    End Sub

