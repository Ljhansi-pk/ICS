    'Changed as per Incident # 31208 and Change Order # 6074
    Private Sub AddProductDataToTRACSData(ByVal oSet As System.Data.DataSet, ByRef stSKU As ICS.Datasets.SKUtoICSDataset)

        Dim i As Int16
        '   Add TreadWear Indicators Header to Result Set
        If oSet.Tables(0).TableName = "ProductData" Then
            ConvertNulls(oSet.Tables(0))
            For i = 0 To oSet.Tables(0).Rows.Count - 1
                Dim sSKUHdrRow As ICS.Datasets.SKUtoICSDataset.ProductDataRow = stSKU.ProductData.NewProductDataRow
                sSKUHdrRow.SKU = oSet.Tables(0).Rows(i).Item("SKU")
                sSKUHdrRow.BrandDesc = oSet.Tables(0).Rows(i).Item("BRANDDESC")
                sSKUHdrRow.SerialDate = oSet.Tables(0).Rows(i).Item("SERIALDATE")
                sSKUHdrRow.DOTSerialNumber = oSet.Tables(0).Rows(i).Item("DOTSERIALNUMBER")
                sSKUHdrRow.SizeStamp = oSet.Tables(0).Rows(i).Item("SIZESTAMP")
                sSKUHdrRow.SpeedRating = oSet.Tables(0).Rows(i).Item("SPEEDRATING")
                sSKUHdrRow.SingLoadIndex = oSet.Tables(0).Rows(i).Item("SINGLOADINDEX")
                sSKUHdrRow.DualLoadIndex = oSet.Tables(0).Rows(i).Item("DUALLOADINDEX")
                sSKUHdrRow.BiasBeltedRadial = oSet.Tables(0).Rows(i).Item("BIASBELTEDRADIAL")
                sSKUHdrRow.TubelessYN = oSet.Tables(0).Rows(i).Item("TUBELESS")
                sSKUHdrRow.ReinforcedYN = oSet.Tables(0).Rows(i).Item("REINFORCEDYN")
                sSKUHdrRow.ExtraLoadYN = oSet.Tables(0).Rows(i).Item("EXTRALOADYN")
                sSKUHdrRow.UTQGTreadwear = oSet.Tables(0).Rows(i).Item("UTQGTREADWEAR")
                sSKUHdrRow.UTQGTraction = oSet.Tables(0).Rows(i).Item("UTQGTRACTION")
                sSKUHdrRow.UTQGTemp = oSet.Tables(0).Rows(i).Item("UTQGTEMP")
                sSKUHdrRow.MudSnowYN = oSet.Tables(0).Rows(i).Item("MUDSNOWYN")
                sSKUHdrRow.RimDiameter = ConvertToSingle(oSet.Tables(0).Rows(i).Item("RIMDIAMETER"))
                sSKUHdrRow.LoadRange = oSet.Tables(0).Rows(i).Item("LOADRANGE")
                sSKUHdrRow.MeaRimWidth = ConvertToSingle(oSet.Tables(0).Rows(i).Item("MEARIMWIDTH"))
                sSKUHdrRow.RegroovableInd = oSet.Tables(0).Rows(i).Item("REGROOVABLEIND")
                sSKUHdrRow.PlantProduced = oSet.Tables(0).Rows(i).Item("PLANTPRODUCED")
                sSKUHdrRow.MostRecentTestDate = oSet.Tables(0).Rows(i).Item("MOSTRECENTTESTDATE")
                sSKUHdrRow.IMark = oSet.Tables(0).Rows(i).Item("IMARK")
                sSKUHdrRow.PPN = oSet.Tables(0).Rows(i).Item("TECHNICALPLATFORM")
                sSKUHdrRow.AspectRatio = oSet.Tables(0).Rows(i).Item("ASPECTRATIO")
                sSKUHdrRow.MFGWWYY = oSet.Tables(0).Rows(i).Item("MFGWWYY")
                sSKUHdrRow.SevereWeatherInd = oSet.Tables(0).Rows(i).Item("SevereWeatherInd")
                sSKUHdrRow.TireTypeID = oSet.Tables(0).Rows(i).Item("TireTypeID")
                sSKUHdrRow.TreadPattern = oSet.Tables(0).Rows(i).Item("TreadPattern")
                stSKU.ProductData.AddProductDataRow(sSKUHdrRow)
