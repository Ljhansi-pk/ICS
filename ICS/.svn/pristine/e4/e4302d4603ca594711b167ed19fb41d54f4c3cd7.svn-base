    '*********************************************************************
    '*  This routine is used to insure that no nulls are included in the
    '*  data set.  It replaces all nulls in a table with empty strings.
    '*********************************************************************
    Private Sub ConvertNulls(ByRef oTable As System.Data.DataTable)
        Dim nRowIndex As Integer
        Dim nColIndex As Integer

        For nRowIndex = 0 To oTable.Rows.Count - 1
            For nColIndex = 0 To oTable.Columns.Count - 1
                If (oTable.Rows(nRowIndex).Item(nColIndex) Is System.DBNull.Value) Then
                    oTable.Rows(nRowIndex).Item(nColIndex) = ""
                End If
            Next
        Next

    End Sub

