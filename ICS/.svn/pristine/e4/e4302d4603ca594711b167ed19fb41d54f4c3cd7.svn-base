                sTRACSHdrRow.CompletionDate = ConvertToDate(oTable.Rows(rowCount).Item("CompletionDate"))
                sTRACSHdrRow.DOTSerialNumber = oTable.Rows(rowCount).Item("DOTSerialNumber")
                sTRACSHdrRow.SerialDate = oTable.Rows(rowCount).Item("SerialDate")
                sTRACSHdrRow.LowestWearbar = ConvertToSingle(oTable.Rows(rowCount).Item("LowestWearbar"))
                sTRACSHdrRow.PassYN = oTable.Rows(rowCount).Item("PassYN")
                sTRACSHdrRow.Operation = oTable.Rows(rowCount).Item("OperationDescription")
                sTRACSHdrRow.MFGWWYY = oTable.Rows(rowCount).Item("SERIALDATE")
                'sTRACSHdrRow.GTSPEC = oTable.Rows(rowCount).Item("GTSpec")
                sTRACSHdrRow.GTSPEC = ""
                stTRACS.TreadWearHdr.AddTreadWearHdrRow(sTRACSHdrRow)
            Next
        End If

        '   Add TreadWear Indicators Detail to Result Set
        If oTable.TableName = "TreadWearDtl" Then
            ConvertNulls(oTable)
            For rowCount = 0 To oTable.Rows.Count - 1
                Dim sTRACSDtlRow As ICS.Datasets.TRACStoICSDataset.TreadWearDtlRow = stTRACS.TreadWearDtl.NewTreadWearDtlRow
                sTRACSDtlRow.ITERATION = ConvertToShort(oTable.Rows(rowCount).Item("ITERATION"))
                sTRACSDtlRow.WearbarHeight = ConvertToSingle(oTable.Rows(rowCount).Item("WearbarHeight"))
                If oTable.Rows(rowCount).Item("WearbarHeight") < nLowestTreadwearInd Then
                    nLowestTreadwearInd = ConvertToSingle(oTable.Rows(rowCount).Item("WearbarHeight"))
                End If
                stTRACS.TreadWearDtl.AddTreadWearDtlRow(sTRACSDtlRow)
            Next
        End If
        stTRACS.TreadWearHdr(0).LowestWearbar = nLowestTreadwearInd
    End Sub

