    ''' <summary>
    ''' Converts string value to Date.
    ''' </summary>
    ''' <param name="value">value</param>
    ''' <returns>Single</returns>
    ''' <remarks></remarks>
    Private Function ConvertToDate(ByVal value As String) As Date
        Dim convertedValue As Date

        Try
            If (Not String.IsNullOrEmpty(value)) Then

                Select Case value.Trim().Length
                    Case 8
                        convertedValue = Format(DateTime.ParseExact(value, "yyyyMMdd", System.Globalization.CultureInfo.InvariantCulture), "MM/dd/yyyy")
                    Case 14
                        convertedValue = Format(DateTime.ParseExact(value, "yyyyMMddHHmmss", System.Globalization.CultureInfo.InvariantCulture), "MM/dd/yyyy")
                End Select
            End If
        Catch ex As Exception

        End Try

        Return convertedValue
    End Function

    ''' <summary>
    ''' Splits into 2 seperate datasets for Sap And Tracs
    ''' </summary>
    ''' <param name="oSet"></param>
    ''' <param name="oSap"></param>
    ''' <remarks></remarks>
    Private Sub SplitSapAndTracsDataset(ByRef oSet As ICS.Datasets.ClientRequest, ByRef oSap As DataSet)

        Dim index As Integer = 0
        Dim dtSap As New DataTable
        Dim deleteSapItems As New Stack

        dtSap.Columns.Add(New DataColumn("LotNumber"))
        dtSap.Columns.Add(New DataColumn("Operation"))
        dtSap.Columns.Add(New DataColumn("OperationDescription"))
        dtSap.Columns.Add(New DataColumn("TireNumber"))

        If ((Not oSet Is Nothing) AndAlso (oSet.Tables.Count > 0)) Then
            For index = 0 To oSet.Tables(0).Rows.Count - 1

                If (oSet.Tables(0).Rows(index).Item("ProjectNum").ToString().Length = 12 And Not oSet.Tables(0).Rows(index).Item("Operation") Is DBNull.Value) Then
                    Dim drSapNewRow As DataRow = dtSap.NewRow()
                    drSapNewRow.Item("LotNumber") = oSet.Tables(0).Rows(index).Item("ProjectNum")
                    drSapNewRow.Item("TireNumber") = oSet.Tables(0).Rows(index).Item("TireNum")
                    drSapNewRow.Item("Operation") = oSet.Tables(0).Rows(index).Item("Operation")
                    drSapNewRow.Item("OperationDescription") = oSet.Tables(0).Rows(index).Item("TestSpec")
                    dtSap.Rows.Add(drSapNewRow)

                    'Add Sap rowindex
                    deleteSapItems.Push(index)
                End If
            Next

            'Delete Sap rowindexes from the dataset
            While deleteSapItems.Count > 0
                oSet.Tables(0).Rows.RemoveAt(CInt(deleteSapItems.Pop()))
            End While

            oSet.AcceptChanges()
            oSap.Tables.Add(dtSap)

        End If
    End Sub

