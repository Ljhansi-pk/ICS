    Private Sub BuildList()
        Const strCCCSequentialReport As String = "CCC Sequential Report"
        Const strCCCProductDescriptionReport As String = "CCC Product Description Report"
        Const strEmarkPassengerApplication As String = "Emark Passenger Application"
        Const strEmarkLightTruckApplication As String = "Emark Light Truck Application"
        Const strEmarkLightTruckCertification As String = "Emark Light Truck Certification"
        Const strEmarkPassengerCertification As String = "Emark Passenger Certification"
        Const strEmarkE117Report As String = "Emark E117 Report"
        Const strEmarkMSRPassengerTestReport As String = "Emark MSR Passenger Test Report"
        Const strEmarkMETruckTestReport As String = "Emark ME Truck Test Report"
        Const strEmarkMSRTruckTestReport As String = "Emark MSR Truck Test Report"
        Const strECEApprovalNumberswithSimilarMaterialsReport As String = "ECE Approval Numbers with Similar Materials Report"
        Const strGSOPassengerCertification As String = "GSO Passenger Certification"
        Const strGSOTruckCertification As String = "GSO Truck Certification"
        Const strGSOConformityCertificateReport As String = "GSO Conformity Certificate Report"
        Const strImarkCertification As String = "Imark Certification"
        Const strImarkConformityMarkReport As String = "Imark Conformity Mark Report"
        Const strImarkSamplingandTireTests As String = "Imark Sampling and Tire Tests"
        Const strImarkECEAuthenticityReport As String = "Imark ECE Authenticity Report"
        Const strNOM086SCFI2010 As String = "NOM-086-SCFI-2010"
        Const strNOM0861SCFI2011 As String = "NOM-086/1-SCFI-2011"
        Const strMaterialCertification As String = "Material Certification"
        Const strPerformanceReport As String = "Performance Report"
        Const strMaterialMasterICSDifferenceReport As String = "Material Master / ICS Difference Report"

        Try
            _dict = New Dictionary(Of NameAid.Report, String)
            _dict.Add(NameAid.Report.CCCSequentialReport, strCCCSequentialReport)
            _dict.Add(NameAid.Report.CCCProductDescriptionReport, strCCCProductDescriptionReport)
            _dict.Add(NameAid.Report.EmarkPassengerApplication, strEmarkPassengerApplication)
            _dict.Add(NameAid.Report.EmarkLightTruckApplication, strEmarkLightTruckApplication)
            _dict.Add(NameAid.Report.EmarkLightTruckCertification, strEmarkLightTruckCertification)
            _dict.Add(NameAid.Report.EmarkPassengerCertification, strEmarkPassengerCertification)
            _dict.Add(NameAid.Report.EmarkE117Certification, strEmarkE117Report)
            _dict.Add(NameAid.Report.EmarkMSRPassengerReport, strEmarkMSRPassengerTestReport)
            _dict.Add(NameAid.Report.EmarkMSRTruckReport, strEmarkMSRTruckTestReport)
            _dict.Add(NameAid.Report.EmarkMETruckReport, strEmarkMETruckTestReport)

            'Changed report name as per PRJ3617 SAP Interface to International Certification System (OPQ.I.8265)Remediation
            _dict.Add(NameAid.Report.EmarkSimilarCertificateSearch, strECEApprovalNumberswithSimilarMaterialsReport)

            _dict.Add(NameAid.Report.GSOPassengerCertification, strGSOPassengerCertification)
            _dict.Add(NameAid.Report.GSOTruckCertification, strGSOTruckCertification)
            _dict.Add(NameAid.Report.GSOConformityCertificateReport, strGSOConformityCertificateReport)
            _dict.Add(NameAid.Report.ImarkCertification, strImarkCertification)
            _dict.Add(NameAid.Report.ImarkConformityMarkReport, strImarkConformityMarkReport)
            _dict.Add(NameAid.Report.ImarkSamplingAndTireTests, strImarkSamplingandTireTests)
            _dict.Add(NameAid.Report.ImarkECEAuthenticityReport, strImarkECEAuthenticityReport)
            _dict.Add(NameAid.Report.NOMPassengerCertification, strNOM086SCFI2010)
            _dict.Add(NameAid.Report.NOMLightTruckCertification, strNOM0861SCFI2011)

            'Changed report name as per PRJ3617 SAP Interface to International Certification System (OPQ.I.8265)Remediation
            _dict.Add(NameAid.Report.SKUCertification, strMaterialCertification)
            _dict.Add(NameAid.Report.TraceabilityReport, strPerformanceReport)

            'Changed report name as per PRJ3617 SAP Interface to International Certification System (OPQ.I.8265)Remediation
            _dict.Add(NameAid.Report.ExceptionReport, strMaterialMasterICSDifferenceReport)
        Catch ex As Exception
